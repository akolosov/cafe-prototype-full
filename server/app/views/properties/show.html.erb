<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @property.name %>
</p>

<p>
  <b>Description:</b>
  <%= @property.description %>
</p>

<div class="btn-group">
 <%= link_to 'Back', properties_path, :class => 'btn btn-small ' %>
 <%= link_to 'Edit', edit_property_path(@property), :class => 'btn btn-small btn-primary' if can? :update, @property %>
</div>

